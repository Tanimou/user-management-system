{
  "name": "user-management-api",
  "version": "1.0.0",
  "description": "Serverless API for user management system",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "echo 'Build complete'",
    "db:migrate": "prisma migrate dev",
    "db:generate": "prisma generate",
    "db:studio": "prisma studio",
    "db:push": "prisma db push",
    "db:seed": "tsx scripts/seed.ts",
    "db:performance": "tsx scripts/performance-test.ts",
    "db:update-admin-password": "tsx scripts/update-admin-password.ts",
    "test:admin-password-api": "tsx scripts/test-admin-password-api.ts",
    "test": "vitest",
    "test:run": "vitest --run",
    "test:watch": "vitest --watch",
    "test:coverage": "vitest --coverage",
    "test:unit": "vitest --run tests/unit",
    "test:integration": "vitest --run tests/integration",
    "test:ui": "vitest --ui",
    "lint": "echo 'ESLint configuration pending - linting will be enabled in next iteration'",
    "lint:fix": "echo 'ESLint configuration pending - linting will be enabled in next iteration'",
    "format": "prettier --write \"**/*.{ts,js,json}\"",
    "format:check": "prettier --check \"**/*.{ts,js,json}\""
  },
  "dependencies": {
    "@prisma/client": "^5.7.1",
    "@types/bcryptjs": "^2.4.6",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/node": "^20.10.6",
    "argon2": "^0.44.0",
    "jsonwebtoken": "^9.0.2",
    "prisma": "^5.7.1",
    "typescript": "^5.3.3"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^7.0.0",
    "@typescript-eslint/parser": "^8.40.0",
    "@typescript-eslint/project-service": "^8.40.0",
    "@typescript-eslint/scope-manager": "^8.40.0",
    "@typescript-eslint/tsconfig-utils": "^8.40.0",
    "@typescript-eslint/types": "^8.40.0",
    "@typescript-eslint/typescript-estree": "^8.40.0",
    "@typescript-eslint/visitor-keys": "^8.40.0",
    "@vercel/node": "5.3.13",
    "@vitest/coverage-v8": "^3.2.4",
    "brace-expansion": "^4.0.1",
    "eslint": "^9.33.0",
    "eslint-plugin-security": "^3.0.0",
    "eslint-visitor-keys": "^4.2.1",
    "minimatch": "^9.0.5",
    "prettier": "^3.1.0",
    "ts-api-utils": "^2.1.0",
    "tsx": "^4.7.0",
    "vitest": "^3.2.4"
  },
  "prisma": {
    "seed": "tsx scripts/seed.ts"
  }
}