{
  "name": "user-management-api",
  "version": "1.0.0",
  "description": "Serverless API for user management system",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "echo 'Build complete'",
    "db:migrate": "prisma migrate dev",
    "db:generate": "prisma generate",
    "db:studio": "prisma studio",
    "db:push": "prisma db push",
    "db:seed": "tsx scripts/seed.ts",
    "test": "vitest",
    "test:coverage": "vitest --coverage",
    "lint": "echo 'ESLint configuration pending - linting will be enabled in next iteration'",
    "lint:fix": "echo 'ESLint configuration pending - linting will be enabled in next iteration'",
    "format": "prettier --write \"**/*.{ts,js,json}\"",
    "format:check": "prettier --check \"**/*.{ts,js,json}\""
  },
  "dependencies": {
    "@prisma/client": "^5.7.1",
    "@types/bcryptjs": "^2.4.6",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/node": "^24.3.0",
    "argon2": "^0.31.2",
    "jsonwebtoken": "^9.0.2",
    "prisma": "^5.7.1",
    "typescript": "^5.3.3"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^7.0.0",
    "@typescript-eslint/parser": "^7.0.0",
    "@vercel/node": "5.3.13",
    "@vitest/coverage-v8": "^3.2.4",
    "eslint": "^9.33.0",
    "eslint-plugin-security": "^3.0.0",
    "prettier": "^3.1.0",
    "tsx": "^4.7.0",
    "vitest": "^3.2.4"
  }
}
